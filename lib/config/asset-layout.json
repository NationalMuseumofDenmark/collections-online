{
  "sections": [
    {
      "title": "Tid",
      "rows": [
        {
          "title": "Optagelsestid",
          "type": "date-interval",
          "fields": {
            "from": "acceptance_time_from",
            "to": "acceptance_time_to"
          }
        }, {
          "title": "Fremstillingstid",
          "type": "date-interval",
          "fields": {
            "from": "creation_time_from",
            "to": "creation_time_to"
          }
        }, {
          "title": "Tidsnote",
          "template": "= timenote"
        }
      ]
    }, {
      "title": "Sted",
      "rows": [
        {
          "title": "Adresse",
          "template": "= address || street"
        }, {
          "title": "By",
          "template": "= city"
        }, {
          "title": "Topnr",
          "template": "= topno"
        }, {
          "title": "Koordinater",
          "type": "map-coordinates",
          "fields": {
            "latitude": "latitude",
            "longitude": "longitude"
          }
        }
      ]
    }, {
      "title": "Yderligere oplysninger",
      "rows": [
        {
          "title": "Samling",
          "template": "!= catalogLinked(catalogs, catalog)"
        },
        {
          "title": "Licens",
          "template": "!= licenseLinked(license)"
        }, {
          "title": "Fotograf/skaber",
          "template": "= creator"
        }, {
          "title": "Kilde",
          "template": "= copyright"
        }, {
          "title": "Arkivalie nr",
          "template": "= archiveno"
        }, {
          "title": "Negativ nr",
          "template": "= negative_number"
        }, {
          "title": "Genstands nr",
          "template": "= object_number"
        }, {
          "title": "Inventar nr",
          "template": "= inventory_number"
        }, {
          "title": "Arkivnavn",
          "template": "= archivename ? archivename.displaystring : ''"
        }
      ]
    }, {
      "title": "Tekniske oplysninger",
      "rows": [
        {
          "title": "Fil-ID",
          "template": "= id"
        }, {
          "title": "Filnavn",
          "template": "= filename"
        }, {
          "title": "Filstørrelse (mb)",
          "template": "= filesizeMB(file_size)"
        }, {
          "title": "Størrelse (pixel)",
          "template": "!= decimals(width_px, 0) + ' &times; ' + decimals(height_px, 0)"
        }, {
          "title": "Størrelse (cm)",
          "template": "!= decimals(width_cm, 1) + ' &times; ' + decimals(height_cm, 1)"
        }, {
          "title": "Opløsning (DPI)",
          "template": "= decimals(dpi, 0)"
        }
      ]
    }
  ]
}
