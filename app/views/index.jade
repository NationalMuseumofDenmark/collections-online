include mixins
- var pageTitle = 'Søgeresultat'
- var pageHeader = 'Din søgning resulterede i følgende resultater'
doctype html
html(lang='da').no-js
  head
    +head()
    <!-- build:css /styles/main.css -->
    link(rel='stylesheet', href='../styles/main.css')
    <!-- endbuild -->
    block header
    +analytics()
    <!--[if lt IE 9]><script src="/bower_components/html5shiv/dist/html5shiv.js"></script><![endif]-->

  body
    +sidebarmenu()
    div.main-wrapper
      +header()
      .container
        block content
      +footer()
      a#toTop

    <!-- build:js /scripts/main.js -->
